        <!--打印出sql-->
        <setting name="logImpl" value="STDOUT_LOGGING"/>
    </settings>

    <plugins>
        <plugin interceptor="com.Berries.Wang.mybatis.plugin.STUMyBatisPlugin"/>
    </plugins>
</configuration>
